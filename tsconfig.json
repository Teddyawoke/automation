{
    "compilerOptions": {
      "target": "ES2019",
      "lib": ["esnext"],
      "experimentalDecorators": true,
      "noImplicitAny": true,
      "strict": true,
      "rootDir": "./src",
      "outDir": "./dist",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "noEmit": false,
      "resolveJsonModule": true
    },
    "include": ["src/**/*", "src/appsscript.json"]
  }
  